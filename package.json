{
  "name": "location-based-pwa",
  "private": true,
  "version": "1.0.0",
  "description": "LocationBased-PWA",
  "repository": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "check-types": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.cjs",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "dom7": "^4.0.6",
    "framework7": "^8.0.5",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.0.5",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "material-icons": "^1.13.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^9.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "1.9.3",
    "@types/leaflet-routing-machine": "^3.2.4",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.0",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.4.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "workbox-cli": "^7.0.0"
  }
}
